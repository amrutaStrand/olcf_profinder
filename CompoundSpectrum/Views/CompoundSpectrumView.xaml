<UserControl 
    x:Class="Agilent.OpenLab.CompoundSpectrum.CompoundSpectrumView" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Height="Auto" 
    Width="Auto" 
    Margin="0" 
    Focusable="True"
    Loaded="OnLoaded" 
    Unloaded="OnUnloaded">
    <Grid x:Uid="plotGrid" Name="plotGrid">
        <WindowsFormsHost Focusable="True" x:Uid="plotControlHost" Name="plotControlHost" Grid.Row="0" Grid.Column="0" Panel.ZIndex="1"></WindowsFormsHost>
    </Grid>
</UserControl>